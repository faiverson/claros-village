{
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-tailwindcss"
  ],
  "organizeImportsSkipDestructiveCodeActions": true,
  "singleQuote": true,
  "semi": false,
  "bracketSpacing": true,
  "printWidth": 150,
  "trailingComma": "all",
  "tabWidth": 4,
  "useTabs": false,
  "endOfLine": "auto"
}
